@model LubriXpress.Entities.RegistrationEnt

@{
    ViewBag.Title = "Matricula";
    Layout = "~/Views/Shared/_Layout_Administration.cshtml";
}



@using (Html.BeginForm("CreateRegistration", "Registration", FormMethod.Post, new { id = "RegistrationForm" }))
{
    <main id="main" class="main">

        <div class="pagetitle">
            <h1>Crear Matricula</h1>
            <nav>
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="/Administration/">Inicio</a></li>
                    <li class="breadcrumb-item"><a href="#">Matricula</a></li>
                </ol>
            </nav>
        </div><!-- End Page Title -->



        <section class="section">
            <div class="row">

                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">Matricula</h5>
                        @if (ViewBag.MsjPantalla != null)
                        {
                            <div id="MsjPantalla" style="text-align:center" class="alert alert-danger" role="alert">
                                @ViewBag.MsjPantalla
                            </div>
                        }
                        @if (ViewBag.MsjPantallaP != null)
                        {
                            <div id="MsjPantalla" style="text-align:center" class="alert alert-success" role="alert">
                                @ViewBag.MsjPantallaP
                            </div>
                        }
                        <div class="row">
                            <div class="col-lg-6">
                                <!-- General Form Elements -->
                                <!-- Row 1 -->
                                <div class="row">
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <label for="personalIDField" class="col-form-label">Cedula</label>
                                            @Html.TextBoxFor(m => m.PersonalID, new { @type = "text", @placeholder = "Digite la Cédula", @class = "form-control", id = "PersonalId", @Value = @Session["CedulaCliente"].ToString() })
                                            <div class="validation-message text-danger" id="validationMessage"></div>
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <label for="nameField" class="col-form-label">Curso</label>
                                            @Html.DropDownListFor(m => m.CourseId, ViewBag.Course as List<SelectListItem>, "Selecione el Curso", new { @class = "form-control", @id = "ListaCursos" })

                                        </div>
                                    </div>
                                </div>


                                <!-- Row 2 -->

                                <div class="row">
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <label for="" class="col-form-label">Modalidad</label>
                                            @Html.DropDownListFor(m => m.ModalityId, ViewBag.Modality as List<SelectListItem>, "Selecione la Modalidad", new { @class = "form-control", id = "ListaModalidad" })
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <label for="" class="col-form-label">Nivel</label>
                                            @Html.DropDownListFor(m => m.LevelCourseId, ViewBag.Level as List<SelectListItem>, "Selecione el nivel", new { @class = "form-control", id = "ListaNivel" })
                                        </div>
                                    </div>
                                </div>


                                <!-- Row 3 -->

                                <div class="row">
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <label for="" class="col-form-label">Horario</label>
                                            @Html.DropDownListFor(m => m.ScheduleId, ViewBag.Schedule as List<SelectListItem>, "Selecione el horario", new { @class = "form-control", id = "ListaHorario" })
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <label for="" class="col-form-label">Monto Mensual</label>
                                            @Html.TextBoxFor(m => m.Amount, new { @type = "text", @placeholder = "Digite el Monto", @class = "form-control", id = "Monto" })
                                            <div class="validation-message text-danger" id="ValidationMessageMonto"></div>
                                        </div>
                                    </div>
                                </div>


                                <!-- Row 4 -->

                                <div class="row">
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <label for="" class="col-form-label">Fecha de Matricula</label>
                                            @Html.TextBoxFor(m => m.RegistrationDate, new { @type = "date", @class = "form-control", id = "fechamatricula" })
                                            <div class="validation-message text-danger" id="registValidationMessage"></div>
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <label class="col-form-label">Dia de Pago</label>
                                            @Html.TextBoxFor(m => m.PaymentDay, new { @type = "Number", min = "1", max = "31", @class = "form-control", id = "DayPay" })
                                            <div class="validation-message text-danger" id="validationMessageDayPay"></div>
                                        </div>
                                    </div>
                                </div>


                                <!-- Row 5 -->

                                <div class="row mb-4">
                                    <div class="col-lg-12">
                                        <div class="form-group">
                                            <label class="col-form-label">Comentario</label>
                                            @Html.TextAreaFor(m => m.Comments, new { @type = "Text", @placeholder = "Digite un comentario", @class = "form-control", @style = "height: 100px", id = "Comment" })

                                        </div>
                                    </div>
                                </div>


                                <!-- Row 6 -->

                                <div class="row">
                                    <div class="col-lg-6 order-lg-2">
                                        <button type="button" class="btn m-0 w-100 text-white" style="background-color: #5f81e8" data-bs-toggle="modal" data-bs-target="#basicModal" id="RegistrationFormButton">Ingresar Matricula</button>

                                    </div>
                                    <div class="col-lg-6 order-lg-1">
                                        <button type="button" class="btn btn-secondary m-0 w-100" onclick="window.history.back();">Regresar</button>

                                    </div>
                                </div>

                            </div>

                            <div class="col-lg-6" style="text-align: center; display: flex; flex-direction: column; justify-content: center; align-items: center;">
                                <img src="data:image/jpeg;base64," alt="Imagen del Centro Cultural de Idiomas" style="width: 50%; height: auto;">
                            </div>

                        </div>
                    </div>
                </div>
            </div>

        </section>


        <!-- successModal -->
        <div class="modal fade" id="successModal" tabindex="-1">
            <div class="modal-dialog justify-content-center align-items-center" style="margin-top: 15%">
                <div class="modal-content justify-content-center align-items-center">
                    <div class="modal-header align-items-center">
                        <h5 class="modal-title justify-content-center align-items-center"><i class="bi bi-award-fill text-success"></i>  Alerta  <i class="bi bi-award-fill text-success"></i></h5>
                    </div>
                    <div class="modal-body" style="text-align:center">
                        ¿Desea crear la matricula?
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-danger" data-bs-dismiss="modal">Cancelar</button>
                        <button type="submit" class="btn btn-success">Crear</button>
                    </div>
                </div>
            </div>
        </div>
        <!-- End Modal-->
        <!-- errorModal -->
        <div class="modal fade" id="errorModal" tabindex="-1">
            <div class="modal-dialog justify-content-center align-items-center" style="margin-top: 15%">
                <div class="modal-content justify-content-center align-items-center">
                    <div class="modal-header align-items-center">
                        <h5 class="modal-title justify-content-center align-items-center"><i class="bi bi-x-octagon text-danger"></i>  Alerta  <i class="bi bi-x-octagon text-danger"></i></h5>
                    </div>
                    <div class="modal-body" style="text-align:center">
                        Por favor llene todos los campos resaltados en rojo
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Atras</button>
                    </div>
                </div>
            </div>
        </div>
        <!-- End Modal-->



    </main><!-- End #main -->
}

<script src="~/assets/Administration/js/RegistrationValidation.js"></script>